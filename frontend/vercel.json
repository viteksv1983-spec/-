{
    "crons": [
        {
            "path": "/api/regenerate-sitemap",
            "schedule": "0 3 * * *"
        }
    ],
    "rewrites": [
        {
            "source": "/(login|register|cart|promotions|delivery|fillings|about|gallery/.*|reviews|blog|districts/.*|cakes|cakes/.*|admin|admin/.*)",
            "destination": "/index.html"
        }
    ]
}