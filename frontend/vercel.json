{
    "crons": [
        {
            "path": "/api/regenerate-sitemap",
            "schedule": "0 3 * * *"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": true,
    "rewrites": [
        {
            "source": "/(login|register|cart|admin|admin/.*)",
            "destination": "/index.html"
        }
    ]
}